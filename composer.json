{
    "name": "conversionia/leadflex",
    "description": "Provides specialized LeadFlex utilities.",
    "type": "craft-plugin",
    "version": "4.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "leadflex"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Conversion Interactive Agency",
            "homepage": "https://conversionia.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0",
        "nystudio107/craft-typogrify": "^4.0"
    },
    "autoload": {
        "psr-4": {
          "conversionia\\leadflex\\": "src/"
        }
    },
    "extra": {
        "name": "LeadFlex",
        "handle": "leadflex",
        "class": "conversionia\\leadflex\\Leadflex"
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        }
    }
}
